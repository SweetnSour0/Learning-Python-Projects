import random
num = random.randint(1,3)
question = input('Please ask a "Yes or No" question. ')
if num == 1:
  answer = 'Yes - definitely.'
elif num == 2:
  answer = 'It is decidedly so.'
elif num == 3:
  answer = 'Without a doubt.'
else:
  answer = 'Error'

print('Answer: ' + answer )
